{
  "name": "rdme",
  "version": "9.0.0-yooo",
  "description": "ReadMe's official CLI and GitHub Action.",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/readmeio/rdme.git"
  },
  "bugs": {
    "url": "https://github.com/readmeio/rdme/issues"
  },
  "type": "module",
  "oclif": {
    "bin": "rdme",
    "dirname": "rdme",
    "commands": {
      "strategy": "explicit",
      "target": "./cmds.js",
      "identifier": "COMMANDS"
    },
    "topicSeparator": ":",
    "hooks": {
      "prerun": {
        "target": "./cmds.js",
        "identifier": "prerun"
      },
      "createGHA": {
        "target": "./cmds.js",
        "identifier": "createGHA"
      }
    },
    "topics": {
      "categories": {
        "description": "List or create categories in your ReadMe developer hub."
      },
      "docs": {
        "description": "Sync or prune Guides pages in your ReadMe developer hub."
      },
      "openapi": {
        "description": "Manage your API definition. Supports OpenAPI, Swagger, Postman."
      },
      "versions": {
        "description": "Manage your documentation versions."
      }
    }
  }
}
