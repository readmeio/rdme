  },
  "main": "index.js",
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "cli-table": "^0.3.1",
    "colors": "^1.1.2",
    "command-line-args": "^5.1.1",
    "command-line-usage": "^6.0.2",
    "config": "^3.1.0",
    "configstore": "^5.0.0",
    "editor": "^1.0.0",
    "enquirer": "^2.3.0",
    "gray-matter": "^4.0.1",
    "isemail": "^3.1.3",
    "oas": "^2.0.0",
    "open": "^7.0.0",
    "read": "^1.0.7",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "semver": "^7.0.0",
    "table-layout": "^1.0.0"
  },
  "devDependencies": {
    "@readme/eslint-config": "^1.3.1",
    "conventional-changelog-cli": "^2.0.31",
    "eslint": "^6.2.0",
    "jest": "^25.1.0",
    "nock": "^12.0.0",
    "prettier": "^1.12.1"
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint && npm run prettier",
    "prettier": "prettier --list-different --write \"./**/**.js\"",
    "release": "conventional-changelog -i CHANGELOG.md -s",
    "test": "jest --coverage"
  },
  "jest": {
    "setupFiles": [
      "./test/set-node-env"
    ],
    "coverageThreshold": {
      "global": {
